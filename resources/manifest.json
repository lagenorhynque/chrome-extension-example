{
  "manifest_version": 2,
  "name": "chrome-extension-example",
  "version": "0.1",
  "description": "Chrome Extension example",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  }
}
